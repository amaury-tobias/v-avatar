{
  "name": "@amaury-tobias/v-avatar",
  "version": "0.0.3",
  "description": "Componente de abatar basado en Vue.js",
  "author": "amaury.tobiasqr@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amaury-tobias/v-avatar.git"
  },
  "keywords": [
    "avatar",
    "user",
    "vue"
  ],
  "main": "dist/v-avatar.ssr.js",
  "module": "dist/v-avatar.esm.js",
  "unpkg": "dist/v-avatar.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepublish": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
