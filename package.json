{
  "name": "@amaury-tobias/v-avatar",
  "version": "1.0.3",
  "description": "Componente de avatar basado en Vue.js",
  "homepage": "https://amaury-tobias.github.io/v-avatar/",
  "author": "amaury.tobiasqr@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amaury-tobias/v-avatar.git"
  },
  "keywords": [
    "avatar",
    "user",
    "vue",
    "vue-component"
  ],
  "main": "dist/v-avatar.ssr.js",
  "module": "dist/v-avatar.esm.js",
  "unpkg": "dist/v-avatar.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "package.json",
    "README",
    "CHANGELOG",
    "LICENSE"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/v-avatar.vue",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepublish": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-unit-mocha": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "minimist": "^1.2.0",
    "rollup": "^1.19.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
